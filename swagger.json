  "definitions" : {
    "GerarRemessa" : {
      "type" : "object",
      "required" : [ "config", "registros" ],
      "properties" : {
        "config" : {
          "type" : "object",
          "required" : [ "banco", "cnab", "carteira", "agencia", "contaCorrente", "cedenteNome", "sequencialRemessa" ],
          "properties" : {
            "banco" : {
              "type" : "string",
              "description" : "Código do banco com zero a esquerda se houver, bancos homologados:\n   * `001` - Banco do Brasil - CNAB 240\n   * `033` - Santander - CNAB 400\n   * `237` - Bradesco - CNAB 400\n   * `341` - Itaú - CNAB 400\n   * `422` - Banco Safra - CNAB 400\n",
              "example" : "033"
            },
            "cnab" : {
              "type" : "number",
              "description" : "Código do CNAB:\n  * `400` \n  * `240`\n",
              "example" : 400
            },
            "carteira" : {
              "type" : "number",
              "description" : "Número da carteira",
              "example" : 101
            },
            "variacaoCarteira" : {
              "type" : "string",
              "description" : "Nº de variação da carteira se houver",
              "example" : "019"
            },
            "cedente" : {
              "type" : "string",
              "description" : "Nº de cedente, informação obtida no banco",
              "example" : "00112233"
            },
            "agencia" : {
              "type" : "string",
              "description" : "Nº Agência Bancária sem o dígito",
              "example" : "0123"
            },
            "digitoAgencia" : {
              "type" : "string",
              "description" : "Nº do dígito Agência Bancária",
              "example" : "0"
            },
            "contaCorrente" : {
              "type" : "string",
              "description" : "Nº da conta corrente sem o dígito",
              "example" : "0123456"
            },
            "digitoContaCorrente" : {
              "type" : "string",
              "description" : "Nº do dígito da conta corrente",
              "example" : "1"
            },
            "cedenteNome" : {
              "type" : "string",
              "description" : "Nome do cedente, nome da sua empresa:",
              "example" : "Escola infantil Aprender LTDA"
            },
            "cedenteCpfCnpj" : {
              "type" : "string",
              "description" : "CPF/CNPJ do cedente/empresa. Apenas números",
              "example" : "35234582000155"
            },
            "cedenteLogradouro" : {
              "type" : "string",
              "description" : "Endereço e número do cedente/empresa",
              "example" : "Rua nove de Julho 72"
            },
            "cedenteBairro" : {
              "type" : "string",
              "description" : "Bairro do cedente/empresa",
              "example" : "Santo Amaro"
            },
            "cedenteCidade" : {
              "type" : "string",
              "description" : "Cidade do cedente/empresa",
              "example" : "São Paulo"
            },
            "cedenteEstado" : {
              "type" : "string",
              "description" : "Estado do cedente/empresa, 2 caracteres",
              "example" : "SP",
              "minLength" : 2,
              "maxLength" : 2
            },
            "cedenteCep" : {
              "type" : "string",
              "description" : "CEP do cedente/empresa, 8 caracteres, apenas números",
              "example" : "12345678",
              "minLength" : 8,
              "maxLength" : 8
            },
            "sequencialRemessa" : {
              "type" : "number",
              "description" : "Número sequencial da remessa",
              "example" : 50,
              "minimum" : 1,
              "maximum" : 999999
            }
          }
        },
        "registros" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "numeroDocumento" : {
                "type" : "string",
                "description" : "Número do documento (boleto, fatura, etc)",
                "example" : "1234"
              },
              "nossoNumero" : {
                "type" : "string",
                "description" : "Nosso Número",
                "example" : "6565849"
              },
              "dataDocumento" : {
                "type" : "string",
                "description" : "Data de geração do documento no formato DDMMYY.\nExemplo para a data: 10/02/2022\n",
                "example" : "100222"
              },
              "dataVencimento" : {
                "type" : "string",
                "description" : "Data de vencimento do documento no formato DDMMYY.\nExemplo para a data: 15/02/2022\n",
                "example" : "150222"
              }
            }
          }
        }
      }
    },
    "AuthError" : {
      "properties" : {
        "name" : {
          "type" : "string",
          "description" : "Nome do erro",
          "example" : "Unauthorized"
        },
        "message" : {
          "type" : "string",
          "description" : "Descrição do erro",
          "example" : "Your request was made with invalid credentials."
        },
        "code" : {
          "type" : "string",
          "description" : "Código do erro",
          "example" : "0"
        },
        "status" : {
          "type" : "number",
          "description" : "Código de status HTTP",
          "example" : 401
        }
      }
    }
  },
  "schemes" : [ "https" ],
  "securityDefinitions" : {
    "basicAuth" : {
      "type" : "basic"
    }
  },
